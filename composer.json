{
    "name": "datingvip/api",
    "type": "library",
    "description": "DatingVIP API Client",
    "keywords": ["datingvip", "api client"],
    "homepage": "https://github.com/DatingVIP/api-client",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Boris Momčilović",
            "email": "boris@firstbeatmedia.com",
            "homepage": "http://firstbeatmedia.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.3.0",
        "datingvip/curl": "^2.0"
    },
    "require-dev": {
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "DatingVIP\\API\\": "src/DatingVIP/API/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DatingVIP\\API\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
